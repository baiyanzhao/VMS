<Application x:Class="VMS.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:VMS"
             xmlns:tb="http://www.hardcodet.net/taskbar"
             StartupUri="View/MainWindow.xaml" ShutdownMode="OnMainWindowClose" SessionEnding="App_SessionEnding">
	<Application.Resources>
		<ResourceDictionary>
			<local:EmptyStringConvert x:Key="EmptyStringConvert"/>
			<local:StringMatchConverter x:Key="StringMatchConverter"/>
			<local:BranchDetailConverter x:Key="BranchDetailConverter"/>
			<tb:TaskbarIcon x:Key="Taskbar" ToolTipText="VMS" IconSource="VMS.ico"/>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="Style/ButtonStyle.xaml" />
				<ResourceDictionary Source="Style/TabControl.xaml" />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
